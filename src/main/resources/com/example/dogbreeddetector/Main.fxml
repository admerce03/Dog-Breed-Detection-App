<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="statusLabel" prefHeight="465.0" prefWidth="314.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dogbreeddetector.MainController">
   <children>
      <ImageView fx:id="imageView" fitHeight="200.0" fitWidth="200.0" layoutX="59.0" layoutY="33.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="label" alignment="CENTER" layoutX="57.0" layoutY="242.0" prefHeight="58.0" prefWidth="200.0">
         <font>
            <Font name="Verdana" size="12.0" />
         </font>
      </Label>
      <Button fx:id="uploadButton" layoutX="102.0" layoutY="314.0" mnemonicParsing="false" onAction="#handleUploadAction" prefHeight="48.0" prefWidth="116.0" styleClass="button-style" text="upload" />
      <Button fx:id="selectFileButton" layoutX="122.0" layoutY="380.0" mnemonicParsing="false" onAction="#handleSelectFileAction" prefHeight="25.0" prefWidth="75.0" styleClass="button-style" text="select" />
   </children>
</AnchorPane>
